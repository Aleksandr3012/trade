mixin sDocuments(data)
	// start sDocuments
	+b.SECTION.sDocuments.section#sDocuments&attributes(attributes)
		.container
			+b.section-title
				h2 Все компетенции автора подтверждены #[span документально]
				
			.row
				mixin img(i)
					.col-6.col-lg-3
						+e.imgWrap(data-fslightbox="atestat" href=`img/@2x/atestat-${i}.png`)
							+pic(`atestat-${i}.png`)
							+e.iconWrap
								+icon('zoom')
				+img(1)
				+img(2)
				+img(3)
				+img(4)
	// end sDocuments